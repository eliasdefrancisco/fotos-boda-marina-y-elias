{
  "extends": "astro/tsconfigs/base",
	"compilerOptions": {
    "target": "ESNext", 
    "module": "CommonJS", 
    "resolveJsonModule": true,
    "outDir": "./dist",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {},
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
